<%- include('partials/header') -%>
<h1>Home</h1>
<p><%= startingContent %></p>
<% postItems.forEach(function(item) { %>
  <% if(item.title == "Home") { %>
   <h1><%= item.title %></h1>
   <p><%= item.body %></p>
 <% } else { %>
   <h1><%= item.title %></h1>
   <p><%= item.body.substring(0,100) + " ..." %><a href="/posts/<%= item.title %>">Read More</a></p>
 <% } %>
<%  }); %>
<%- include('partials/footer') -%>
